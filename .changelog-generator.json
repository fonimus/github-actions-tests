{
    "categories": [
      {
        "title": "## üöÄ Features",
        "labels": ["feature", "enhancement"]
      },
      {
        "title": "## üêõ Fixes",
        "labels": ["fix", "bug"]
      },
      {
        "title": "## ‚¨ÜÔ∏è Dependencies",
        "labels": ["dependencies"]
      },
      {
        "title": "## üß™ Tests",
        "labels": ["test"]
      },
      {
        "title": "## üìù Documentation",
        "labels": ["documentation"]
      }
    ],
    "ignore_labels": [
      "ignore"
    ],
    "sort": "ASC",
    "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>",
    "pr_template": "- PR : #${{NUMBER}} - ${{TITLE}}\n    - Author: ${{AUTHOR}}\n    - Reviewers: ${{REVIEWERS}}\n    - Body: ${{BODY}}",
    "empty_template": "No changes",
    "label_extractor": [
      {
        "pattern": "(.) (.+)",
        "target": "$1"
      },
      {
        "pattern": "(.) (.+)",
        "target": "$1",
        "on_property": "title"
      }
    ],
    "transformers": [
      {
        "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
        "target": "- $4\n  - $6"
      }
    ],
    "max_tags_to_fetch": 200,
    "max_pull_requests": 200,
    "max_back_track_time_days": 365,
    "exclude_merge_branches": [
      "Owner/qa"
    ],
    "tag_resolver": {
      "method": "semver"
    }
}
